<!DOCTYPE html>
<html lang="en">
    <head>
        <title>MIPS</title>
        <link href="../../Styles/codemirror.css" rel="stylesheet" />
        <link href="../../Styles/ambiance.css" rel="stylesheet" />
        <link href="../../Styles/app.css" rel="stylesheet" />
        <script src="../../Scripts/jquery-1.8.2.js" type="text/javascript"></script>
        <script src="../../Scripts/underscore.js" type="text/javascript"></script>
        <script src="../../Scripts/knockout-2.1.0.debug.js" type="text/javascript"></script>
        <script src="../../Scripts/codemirror.js" type="text/javascript"></script>
        <script src="../../Scripts/assembly.js" type="text/javascript"></script>
        <script src="../../Scripts/app.js" type="text/javascript"></script>
    </head>
    <body>
        <div id="content">
            <div class="input">
                <textarea id="view" rows="0" cols="0">add $t0, $t1, $t2
addi $t0, $t1, -32768
ori $t0, $t1, 32767</textarea>
            </div>
            
            <div class="output cm-s-ambiance">
                <table>
                    <tbody data-bind="foreach: commands">
                        <!--template for r-type commands-->
                        <tr data-bind="visible: type() === 'rtype'" class="captions">
                            <td>op</td>
                            <td>rs</td>
                            <td>rt</td>
                            <td>rd</td>
                            <td>shamt</td>
                            <td>funct</td>
                        </tr>
                        <tr data-bind="visible: type() === 'rtype'" class="code">
                            <td data-bind="text: op"></td>
                            <td data-bind="text: rs"></td>
                            <td data-bind="text: rt"></td>
                            <td data-bind="text: rd"></td>
                            <td data-bind="text: shamt"></td>
                            <td data-bind="text: funct"></td>
                        </tr>
                        <!--template for i-type commands-->
                        <tr data-bind="visible: type() === 'itype'" class="captions">
                            <td>op</td>
                            <td>rs</td>
                            <td>rt</td>
                            <td colspan="3">imm</td>
                        </tr>
                        <tr data-bind="visible: type() === 'itype'" class="code">
                            <td data-bind="text: op"></td>
                            <td data-bind="text: rs"></td>
                            <td data-bind="text: rt"></td>
                            <td data-bind="text: imm" colspan="3"></td>
                        </tr>
                        <!--template for j-type commands-->
                        <tr data-bind="visible: type() === 'jtype'" class="captions">
                            <td>op</td>
                            <td colspan="5">addr</td>
                        </tr>
                        <tr data-bind="visible: type() === 'jtype'" class="code">
                            <td data-bind="text: op"></td>
                            <td data-bind="text: addr" colspan="5"></td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </body>
</html>

